{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "MyOS2 debug",
      "type": "cppdbg",
      "request": "launch",
      // "preLaunchTask": "startup vmw",
      "program": "${workspaceRoot}/build/kernel.debug",
      "miDebuggerServerAddress": "localhost:8864",
      "stopAtEntry": false,
      "cwd": "${workspaceRoot}",
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "miDebuggerArgs": "",
      "targetArchitecture": "x64",
      // "customLaunchSetupCommands": [
      //     {
      //         "text": "target remote localhost:1234",
      //         "description": "Connect to QEMU remote debugger"
      //     }
      // ],
      "setupCommands": [
        {
          "text": "set arch i386:x86-64:intel",
          "ignoreFailures": false
        },
        {
          "text": "dir .",
          "ignoreFailures": false
        },
        // {
        //   "text": "add-symbol-file ${workspaceRoot}/build/init.debug 0x6000000",
        //   "ignoreFailures": false
        // },
        {
          "text": "add-symbol-file ${workspaceRoot}/build/shell.debug 0x6000000",
          "ignoreFailures": false
        },
        {
          "text": "add-auto-load-safe-path ./",
          "ignoreFailures": false
        },
        {
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
      ]
    },
    {
      "type": "bashdb",
      "request": "launch",
      "name": "Bash-Debug (select script from list of sh files)",
      "cwd": "${workspaceFolder}",
      "program": "${command:SelectScriptName}",
      "args": ["asus"]
    },
  ]
}